#include <stdio.h>

void find_divisors(int n) {
    printf("The divisors of %d are: ", n);
    for (int i = 1; i <= n; i++) {
        if (n % i == 0) {
            printf("%d ", i);
        }
    }
    printf("\n");
}

int main() {
    int n;

    printf("Enter a positive integer: ");
    scanf("%d", &n);

    find_divisors(n);

    return 0;
}
