#include <stdio.h>
 struct book {
     char title[50];
     char author[50];
     char subject[100];
     int book_id;
 };
 void display(struct book b) {
     printf("Title: %s\n", b.title);
     printf("Author: %s\n", b.author);
     printf("Subject: %s\n", b.subject);
     printf("Book ID: %d\n", b.book_id);
 }
 int main() {
     struct book b1 = {
         "The Great Gatsby",
         "F. Scott Fitzgerald",
         "Novel",
         1001
     };
     printf("Book 1:\n");
     display(b1);
     return 0;
 }
