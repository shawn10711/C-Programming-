#include <stdio.h>
 int main()
 {
     int n, i, sum = 0;
  printf("Enter a positive integer: ");
     scanf("%d", &n);
  for (i = 1; i <= n; ++i)
     {
         if (n % i == 0)
         {
             sum += i;
         }
     }
  printf("Sum of all positive integer factors of %d = %d", n, sum);
 return 0;
 }
